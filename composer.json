{
    "name": "chandra-hemant/htkc-utils-excel",
    "description": "HTKC Utils Excel library for Excel Export in laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
      "laravel",
      "php",
      "phpspreadsheet",
      "phpexcel",
      "excel",
      "csv",
      "export",
      "import",
      "batch"
    ],
    "autoload": {
        "psr-4": {
            "ChandraHemant\\HtkcUtilsExcel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Hemant Kumar Chandra",
            "email": "hemantchandra242@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.4 || ^8.0 || ^8.1 || ^8.2 || ^8.3",
        "phpoffice/phpspreadsheet": "^1.18",
        "illuminate/support": "5.8.*||^6.0||^7.0||^8.0||^9.0||^10.0||^11.0",
        "psr/simple-cache": "^1.0||^2.0||^3.0",
        "composer/semver": "^3.3"
    },
    "extra": {
      "laravel": {
        "providers": [
          "ChandraHemant\\HtkcUtilsExcel\\ExcelServiceProvider"
        ],
        "aliases": {
          "Excel": "ChandraHemant\\HtkcUtilsExcel\\Facades\\Excel"
        }
      }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
